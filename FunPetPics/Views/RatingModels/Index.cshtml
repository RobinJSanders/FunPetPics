@model IEnumerable<FunPetPics.Models.RatingModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PetID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CutenessRating)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FunynessRating)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AwsomenessRating)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PetID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CutenessRating)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FunynessRating)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AwsomenessRating)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RaitngId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RaitngId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RaitngId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
